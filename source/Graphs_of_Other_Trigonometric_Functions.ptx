              \addplot[stealth-stealth,domain=-pi/2+0.15:pi/2-0.15,samples=200,blue,line width=1pt]{0.5*tan(deg(x))}node[right,pos=0.75]{$y=\frac{1}{2}\tan(x)$};
              \addplot[stealth-stealth,domain=-pi/2+0.3:pi/2-0.3,samples=200,magenta,line width=1pt]{-tan(deg(x))}node[right,pos=0.7]{$y=-\tan(x)$};
              \draw [dashed,black] (-pi/2,-3.3) -- (-pi/2,3.3) node[below,pos=0]{$x=-\frac{\pi}{2}$};
              \draw [dashed,black] (pi/2,-3.3) -- (pi/2,3.3) node[below,pos=0]{$x=\frac{\pi}{2}$};
