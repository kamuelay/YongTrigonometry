              \draw[stealth-] (0,-1.15) -- (0,1);
              \draw[-stealth] (0,0) -- (-45:1.2);
              \draw[-stealth] (0,0) -- (225:1.2);
              \draw[line width=2pt,-stealth] (0,0.9) -- (0,0.5);
              \draw[line width=2pt,-stealth] (0.2,0.9) -- (0.2,0.5);
              \draw[line width=2pt,-stealth] (-0.2,0.9) -- (-0.2,0.5);
