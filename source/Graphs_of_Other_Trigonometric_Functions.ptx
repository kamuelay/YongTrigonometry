          xticklabels={\(-2\pi\),\(-\pi\),,\(\pi\),\(2\pi\),\(3\pi\)},
          xticklabel style={black},
          xlabel=\(x\),
          ylabel=\(y\)]
          \addplot[stealth-stealth,domain=-2.5*pi-0.2:3.5*pi+0.2,samples=200,red,line width=1pt]{cos(deg(x))}node[right,pos=0.8,yshift=7]{\(y =\cos(x)\)};
          \addplot[stealth-stealth,domain=-5*pi/2+0.2:-3*pi/2-0.2,samples=200,blue,line width=1pt]{1/cos(deg(x))};
