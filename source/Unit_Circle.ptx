\draw ([shift=(0:0.2cm)]0,0) arc (0:30:0.2cm) node[midway,right,xshift=0.04cm,yshift=0.1cm] {\(30^{\circ}\)};
\draw ([shift=(210:0.2cm)]30:1cm) arc (210:270:0.2cm) node[midway,below,xshift=-0.1cm] {\(60^{\circ}\)};
