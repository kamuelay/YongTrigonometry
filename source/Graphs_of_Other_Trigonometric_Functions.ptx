          \addplot[stealth-stealth,domain=5*pi/2+0.2:7*pi/2-0.2,samples=200,blue,line width=1pt]{1/cos(deg(x))}node[right,pos=1]{\(y =\sec(x)\)};
          \draw [dashed,red] (-5*pi/2,-5.2) -- (-5*pi/2,5.2) node[below,pos=0]{$x=-\frac{5\pi}{2}$};
          \draw [dashed,red] (-3*pi/2,-5.2) -- (-3*pi/2,5.2) node[above,pos=1]{$x=-\frac{3\pi}{2}$};
          \draw [dashed,red] (-pi/2,-5.2) -- (-pi/2,5.2) node[below,pos=0]{$y=-\frac{\pi}{2}$};
          \draw [dashed,red] (pi/2,-5.2) -- (pi/2,5.2) node[above,pos=1]{$y=\frac{\pi}{2}$};
          \draw [dashed,red] (3*pi/2,-5.2) -- (3*pi/2,5.2) node[below,pos=0]{$y=\frac{3\pi}{2}$};
          \draw [dashed,red] (5*pi/2,-5.2) -- (5*pi/2,5.2) node[above,pos=1]{$y=\frac{5\pi}{2}$};
          \draw [dashed,red] (7*pi/2,-5.2) -- (7*pi/2,5.2) node[below,pos=0]{$y=\frac{5\pi}{2}$};
