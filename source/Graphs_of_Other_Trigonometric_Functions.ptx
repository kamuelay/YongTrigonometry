        <latex-image>
          \begin{tikzpicture}
          \begin{axis}[
          axis lines=middle,clip=false,
          xmin=-pi/2-0.5,xmax=pi/2+0.5,ymin=-2.2,ymax=2.2,
          ytick={-sqrt(3),-1,-sqrt(3)/3,sqrt(3)/3,1,sqrt(3)},
          yticklabels={\(-\sqrt{3}\),\(-1\),\(-\frac{1}{\sqrt{3}}\),\(\frac{1}{\sqrt{3}}\),\(1\),\(\sqrt{3}\)},
          xtick={-pi/2,-pi/3,-pi/4,-pi/6,0,pi/6,pi/4,pi/3,pi/2},
          minor xtick={-3*pi/2,-pi/2,pi/2,3*pi/2,5*pi/2},
          xticklabels={\(-\frac{\pi}{2}\),\(-\frac{\pi}{3}\),\(-\frac{\pi}{4}\),\(-\frac{\pi}{6}\),\(0\),\(\frac{\pi}{6}\),\(\frac{\pi}{4}\),\(\frac{\pi}{3}\),\(\frac{\pi}{2}\)},
