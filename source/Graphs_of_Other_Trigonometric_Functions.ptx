              \addplot[stealth-stealth,domain=-2*pi+0.145:-pi-0.145,samples=200,blue,line width=1pt,draw opacity=0.25]{1/sin(deg(x))};
              \addplot[stealth-stealth,domain=-pi+0.2:0-0.2,samples=200,blue,line width=1pt,draw opacity=0.25]{1/sin(deg(x))};
              \addplot[stealth-stealth,domain=0+0.145:pi-0.145,samples=200,blue,line width=1pt,draw opacity=0.25]{1/sin(deg(x))};
              \addplot[stealth-stealth,domain=pi+0.2:2*pi-0.2,samples=200,blue,line width=1pt,draw opacity=0.25]{1/sin(deg(x))}node[right,pos=1]{\(y =\csc(x)\)};
              \draw [dashed,blue,draw opacity=0.25] (-2*pi,-5.2) -- (-2*pi,7.2);
              \draw [dashed,blue,draw opacity=0.25] (-pi,-5.2) -- (-pi,7.2);
              \draw [dashed,blue,draw opacity=0.25] (0,-5.2) -- (0,7.2);
              \draw [dashed,blue,draw opacity=0.25] (pi,-5.2) -- (pi,7.2);
              \draw [dashed,blue,draw opacity=0.25] (2*pi,-5.2) -- (2*pi,7.2);
              \addplot[stealth-stealth,domain=-2*pi-0.2:2*pi+0.2,samples=200,red,line width=0.5pt,draw opacity=0.25]{sin(deg(x-pi/2))+2}node[right,pos=0.85]{\(y =\sin\left(x-\frac{\pi}{2}\right)+2\)};
              \addplot[stealth-stealth,domain=-2*pi-0.2:-3*pi/2-0.145,samples=200,red,line width=1pt]{1/sin(deg(x-pi/2))+2};
              \addplot[stealth-stealth,domain=-3*pi/2+0.2:-pi/2-0.2,samples=200,red,line width=1pt]{1/sin(deg(x-pi/2))+2};
              \addplot[stealth-stealth,domain=-pi/2+0.145:pi/2-0.145,samples=200,red,line width=1pt]{1/sin(deg(x-pi/2))+2};
              \addplot[stealth-stealth,domain=pi/2+0.2:3*pi/2-0.2,samples=200,red,line width=1pt]{1/sin(deg(x-pi/2))+2}node[right,pos=1]{\(y =\csc\left(x-\frac{\pi}{2}\right)+2\)};
              \addplot[stealth-stealth,domain=3*pi/2+0.145:2*pi+0.2,samples=200,red,line width=1pt]{1/sin(deg(x-pi/2))+2};
              \draw [dashed,red] (-3*pi/2,-5.2) -- (-3*pi/2,7.2);
              \draw [dashed,red] (-pi/2,-5.2) -- (-pi/2,7.2);
              \draw [dashed,red] (pi/2,-5.2) -- (pi/2,7.2);
