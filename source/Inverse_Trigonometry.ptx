            \addplot[domain=-pi/2-0.75:pi/2+0.75,samples=200,orange,line width=1pt,&lt;->]{atan(x)/180*pi}node[right,pos=0.9,yshift=-5,font=\footnotesize]{$f(x)=\tan^{-1} x$};
            \draw[dashed] (-pi/2-1,-pi/2) -- (pi/2+1,-pi/2) node[right] {$y=-\frac{\pi}{2}$};
            \draw[dashed] (-pi/2-1,pi/2) -- (pi/2+1,pi/2) node[right] {$y=\frac{\pi}{2}$};
