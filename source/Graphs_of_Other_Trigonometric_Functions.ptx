    <exercise>
      <statement>
        <p>
          Graph the function <m>d(\theta)</m> on the domain <m>-\frac{\pi}{3}\leq \theta\leq\frac{\pi}{3}</m>.
        </p>
      </statement>
      <answer>
        <image xml:id="exer-faataite-sec">
          <latex-image>
            \begin{tikzpicture}
            \begin{axis}[
            axis lines=middle,clip=false,
            xmin=-pi/3-0.1,xmax=pi/3+0.1,ymin=-0.2,ymax=4.25,
            ytick={1,2,3,4},
            minor ytick={0.5,1.5,...,4},
            xtick={-pi/3,-pi/4,-pi/6,pi/6,pi/4,pi/3},
            xticklabels={\(-\frac{\pi}{3}\),\(-\frac{\pi}{4}\),\(-\frac{\pi}{6}\),\(\frac{\pi}{6}\),\(\frac{\pi}{4}\),\(\frac{\pi}{3}\)},
            xticklabel style={black},
            xlabel=\(\theta\),
            ylabel=\(d\)]
              \addplot[stealth-stealth,domain=-pi/3:pi/3,samples=200,blue,line width=1pt]{1.5*sec(deg(x))}node[right,pos=0.5,yshift=-12]{\(d(x)=1.5\sec(x)\)};
            \end{axis}
            \end{tikzpicture}
          </latex-image>
        </image>
      </answer>
